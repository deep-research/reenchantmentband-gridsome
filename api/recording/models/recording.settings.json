{
  "kind": "collectionType",
  "collectionName": "recordings",
  "info": {
    "name": "recording"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "url": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "multitrack": {
      "type": "boolean",
      "default": true
    },
    "explicit": {
      "type": "boolean"
    },
    "length": {
      "type": "time",
      "required": true
    }
  }
}
